#!/bin/bash
#PBS -l nodes=1:ppn=4,vmem=100gb,walltime=00:45:00
#PBS -N roi-stats
#PBS -V

parc=`jq -r '.parcellation' config.json`

if [ ! -f rois.csv ]; then
	time singularity exec -e docker://brainlife/mcr:r2019a ./compiled/parcRoiStats
fi

if [ ! -f rois.csv ]; then
	echo "output missing" && exit 1
else
	echo "complete" && mkdir -p ./parc-stats/ && mv *.csv ./parc-stats/ && exit 0
fi
